        </Border>
        <Button Width="40" BorderBrush="Black" Panel.ZIndex="2" HorizontalAlignment="Right"  Foreground="White" FontSize="15" Canvas.Left="740" BorderThickness="1" Height="25" VerticalAlignment="Top" Click="Button_Click1" Content="" Margin="0,0,40,0">
            <Button.Background>
                <ImageBrush ImageSource="/free-icon-full-screen-8669501.png"/>
            </Button.Background>
        </Button>
        <Button  Width="40" Background="Transparent" Panel.ZIndex="2" BorderBrush="Black" Foreground="Black" FontSize="15" FontWeight="UltraBold" Canvas.Left="780" BorderThickness="1" Height="25" VerticalAlignment="Top" Click="Button_Click2" HorizontalAlignment="Right" Margin="0,0,80,0" Content="________"/>
